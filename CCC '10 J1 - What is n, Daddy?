def main():

    i = int(input())

    if i == 10:
        print("1")
    elif i == 9:
        print("1")
        
    elif i == 8:
        
        print("2")
        
    elif i == 7:
        
        print("2")
        
    elif i == 6:
        
        print("3")
        
    elif i == 5:
        
        print("3")
        
    elif i == 4:
        
        print("3")
        
    elif i == 3:
        
        print("2")
        
    elif i == 2:
    
        print("2")
        
    elif i == 1:
        
        print("1")
        

main()
